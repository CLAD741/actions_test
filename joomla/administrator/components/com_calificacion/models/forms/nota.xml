<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset addfieldpath="/components/com_calificacion/models/fields">

       <field name="id" type="text" default="0" label="COM_CALIFICACION_FORM_LBL_NOTA_ID"
            readonly="true" class="readonly"
            description="JGLOBAL_FIELD_ID_DESC" /> 

	   <field
			name="nombre"
			type="textfield"
			label="COM_CALIFICACION_NOTA_NOMBRE_LBL"
			description="COM_CALIFICACION_NOTA_NOMBRE_DESC"
			default=""
			/>
	
		<field
			name="correo"
			type="email"
			label="COM_CALIFICACION_NOTA_CORREO_LBL"
			description="COM_CALIFICACION_NOTA_CORREO_DESC"
			default=""
			/>
	
		<field
			name="comentario"
			type="textfield"
			label="COM_CALIFICACION_NOTA_COMENTARIO_LBL"
			description="COM_CALIFICACION_NOTA_COMENTARIO_DESC"
			default=""
			/>
	
		<field
			name="nota"
			type="textfield"
			label="COM_CALIFICACION_NOTA_NOTA_LBL"
			description="COM_CALIFICACION_NOTA_NOTA_DESC"
			default=""
			/>
	
		<field
			name="fecha"
			type="textfield"
			label="COM_CALIFICACION_NOTA_FECHA_LBL"
			description="COM_CALIFICACION_NOTA_FECHA_DESC"
			default=""
			/>
	
		<field
			name="usuario"
			type="user"
			label="COM_CALIFICACION_NOTA_USUARIO_LBL"
			description="COM_CALIFICACION_NOTA_USUARIO_DESC"
			default=""
			/>
	
		<field
			name="created_by"
			type="createdby"
			label="COM_CALIFICACION_NOTA_CREATED_BY_LBL"
			description="COM_CALIFICACION_NOTA_CREATED_BY_DESC"
			default=""
			/>
	
		<field
			class="inputbox"
			size="1"
			name="state"
			type="list"
			label="COM_CALIFICACION_NOTA_STATE_LBL"
			description="COM_CALIFICACION_NOTA_STATE_DESC"
			default="1"
			>
			<option value="1">JPUBLISHED</option>
			<option value="0">JUNPUBLISHED</option>
			<option value="2">JARCHIVED</option>
			<option value="-2">JTRASHED</option>
		</field>
	
		<field
			name="ordering"
			type="hidden"
			label="COM_CALIFICACION_NOTA_ORDERING_LBL"
			description="COM_CALIFICACION_NOTA_ORDERING_DESC"
			default=""
			/>

	</fieldset>
</form>
