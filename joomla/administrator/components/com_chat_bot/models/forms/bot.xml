<?xml version="1.0" encoding="utf-8"?>
<form>
	<fieldset addfieldpath="/components/com_chat_bot/models/fields">

       <field name="id" type="text" default="0" label="COM_CHAT_BOT_FORM_LBL_BOT_ID"
            readonly="true" class="readonly"
            description="JGLOBAL_FIELD_ID_DESC" /> 

	   <field
			name="codrespuesta"
			type="textfield"
			label="COM_CHAT_BOT_BOT_CODRESPUESTA_LBL"
			description="COM_CHAT_BOT_BOT_CODRESPUESTA_DESC"
			default=""
			/>
	
		<field
			name="pregunta"
			type="textfield"
			label="COM_CHAT_BOT_BOT_PREGUNTA_LBL"
			description="COM_CHAT_BOT_BOT_PREGUNTA_DESC"
			default=""
			/>
	
		<field
			name="created_by"
			type="createdby"
			label="COM_CHAT_BOT_BOT_CREATED_BY_LBL"
			description="COM_CHAT_BOT_BOT_CREATED_BY_DESC"
			default=""
			/>
	
		<field
			class="inputbox"
			size="1"
			name="state"
			type="list"
			label="COM_CHAT_BOT_BOT_STATE_LBL"
			description="COM_CHAT_BOT_BOT_STATE_DESC"
			default="1"
			>
			<option value="1">JPUBLISHED</option>
			<option value="0">JUNPUBLISHED</option>
			<option value="2">JARCHIVED</option>
			<option value="-2">JTRASHED</option>
		</field>
	
		<field
			name="ordering"
			type="hidden"
			label="COM_CHAT_BOT_BOT_ORDERING_LBL"
			description="COM_CHAT_BOT_BOT_ORDERING_DESC"
			default=""
			/>

	</fieldset>
</form>
